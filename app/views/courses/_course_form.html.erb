 	<fieldset>
      <%= form_for @course, :html => { :multipart => true } do |f| %>
    
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_area :description, :size => "20x10", placeholder: "add a description here..." %>

      <%= f.label :address %>
      <%= f.text_field :address %>

      <%= f.label :city %>
      <%= f.text_field :city %> 

      <%= f.label :starts_at %>
      <%= f.text_field :starts_at %> 

      <%= f.label :ends_at %>
      <%= f.text_field :ends_at %> 




      <p class="actions">
        <%= f.submit "Add Class", class: 'btn btn-primary' %>
        <%= link_to 'Cancel', courses_path, class: 'btn' %>
      </p>

      <% end %>

  </fieldset>
      
    </div>